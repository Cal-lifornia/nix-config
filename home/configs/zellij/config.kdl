keybinds {//unbind "Alt h" "Alt j" "Alt k" "Alt l"
    unbind "Alt Left" "Alt Down" "Alt Up" "Alt Right"
    normal {
        unbind "Ctrl s"
    }
    shared_except "search" "locked" {
        bind "Ctrl f" {
            SwitchToMode "Search"
        }
    }
}

plugins {
    compact-bar location="zellij:compact-bar" {
        tooltip "F1"
    }
}
default_layout "compact"

theme "catppuccin-macchiato"
show_startup_tips false

