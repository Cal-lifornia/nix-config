keybinds {
  session {
    bind "f" { LaunchOrFocusPlugin "file:~/.config/zellij/plugins/zellij-sessionizer.wasm" {
            floating true
            "move_to_focused_tab" true
            "cwd" "/"
            "root_dirs" "~/projects;~/work;~/personal"
            "session_layout" ":default"
        }
    }
  }
}
